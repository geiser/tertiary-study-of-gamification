@article{Trinidad201831,
  title = {A Systematic Literature Review on the Gamification Monitoring Phase: {{How SPI}} Standards Can Contribute to Gamification Maturity},
  volume = {918},
  issn = {18650929},
  abstract = {Gamification is a novel concept that has attracted the attention of many research fields, including software engineering, software process improvement or software process standards education as an approach to increase productivity, engagement, and motivation of participants involved in a system, site or business. However, gamification is a relative new concept that needs to deal with many problems in order to consolidate and mature its understanding and process. The main goal of this study is to explore the gamification scope with the goal to identify its problems and needs, focusing on the monitoring process of gamification strategies. A systematic literature review was performed following a predefined procedure that involves automatically searching in scientific digital databases. 383 papers were found by the automatic searches in the digital databases and only 2 papers were selected as primary studies. Outcomes show that there is a clear scope of research on the road to provide tools for monitoring high-scalable gamified systems that support gamification experts on gamification analytics and end-users on getting real-time feedback, as well as, a need to mature the gamification process that can be supported by taking advantages from other disciplines, especially from the scope of process capability and maturity. \textcopyright{} Springer Nature Switzerland AG 2018.},
  language = {English},
  journal = {Communications in Computer and Information Science},
  doi = {10.1007/978-3-030-00623-5_3},
  author = {Trinidad, M. and Calder{\'o}n, A. and Ruiz, M.},
  editor = {{Stamelos I., Rout T., O'Connor R.V., Dorling A.}},
  year = {2018},
  keywords = {Assessment,Automatic searches,Circuit simulation,Gamification,Gamification analytics,Monitoring,Monitoring process,Process capabilities,Process engineering,Real time systems,Real-time feedback,Software design,Software Process Improvement,SPI standards,Systematic literature review},
  pages = {31-44},
  file = {/Users/gcc/Google Drive/zotero/storage/LUJ5DAQA/Trinidad et al. - 2018 - A Systematic Literature Review on the Gamification.pdf},
  publisher = {{Springer Verlag}},
  isbn = {9783030006228},
