@inproceedings{O'Donnell2017445,
  series = {{{CHI PLAY}} 2017 {{Extended Abstracts}} - {{Extended Abstracts Publication}} of the {{Annual Symposium}} on {{Computer}}-{{Human Interaction}} in {{Play}}},
  title = {How Multidisciplinary Is Gamification Research? {{Results}} from a Scoping Review},
  isbn = {978-1-4503-5111-9},
  abstract = {Gamification has been repeatedly framed as an emerging multidisciplinary research field. However, it is unclear how multidisciplinary the field actually is. To answer this question, this paper presents initial results of a broader scoping review of gamification research published between 2010 and 2016. Close to 2,000 peer-reviewed English-language journal and conference papers were identified across 11 databases and categorized by discipline. Results indicate an explosive growth of literature peaking in 2015. Early on, Information and Computing Science dominated the field, to be overtaken by the sum of other disciplines in 2013, education, economics and tourism in specific. This indicates that gamification was initially a field within computer science and HCI and has only recently become truly multi-disciplinary. \textcopyright{} 2017 Copyright is held by the owner/author(s).},
  language = {English},
  publisher = {{Association for Computing Machinery, Inc}},
  doi = {10.1145/3130859.3131412},
  author = {O'Donnell, N. and Kappen, D.L. and {Fitz-Walter}, Z. and Deterding, S. and Nacke, L.E. and Johnson, D.},
  year = {2017},
  keywords = {Abstracting,Computing science,Conference papers,English languages,gamification,Gamification,Human computer interaction,Interactive computer systems,Multi-disciplinary research,multidisciplinarity,Multidisciplinarity,scientometrics,Scientometrics,scoping review,Scoping review},
  pages = {445-452},
